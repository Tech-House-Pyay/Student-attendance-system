<!DOCTYPE html>

<html class="app-ui">

    <head>
        <!-- Meta -->
        <% include ../../partial/head %>
        <script src="/assets/js/core/jquery.min.js"></script>
        <!-- End Stylesheets -->
    </head>

    <body class="app-ui">
        <div class="app-layout-canvas">
            <div class="app-layout-container">

                <!-- Header -->
                <header class="app-layout-header">
                    <nav class="navbar navbar-default p-y">
                        <div class="container">
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#header-navbar-collapse" aria-expanded="false">
				<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
                                <% include ../../partial/user-header %>

                <main class="app-layout-content">

                    <!-- Page header -->
                    <div class="page-header bg-app bg-inverse">
                        <div class="container">
                            <!-- Section Content -->
                            <div class="p-y-lg text-center">
                                <h1 class="display-2">Time Table</h1>

                                <!-- Search form -->
                                <!-- <div class="row m-t">
                                    <div class="col-sm-6 col-sm-offset-3">
                                        <form action="frontend_search.html" method="post">
                                            <div class="input-group input-group-lg">
                                                <input class="form-control" type="text" placeholder="Search support.." />
                                                <div class="input-group-btn">
                                                    <button class="btn btn-default"><i class="ion-ios-search-strong"></i></button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div> -->
                                <!-- End search form -->
                            </div>
                            <!-- End Section Content -->
                        </div>
                    </div>
                    <!-- End Page header -->

                    <!-- Categories -->
                    <div class="page-content bg-white">
                        <div class="container">
                    <% include ../../partial/user-submenuT %>
                    <div class="row">
                        <div class="col-sm-10 col-sm-offset-1">
                            <!-- Introduction -->
                            <h2 class="h3 m-t-lg m-b">Time Table</h2>
                            <!-- Classic table -->
                            <h2 class="section-title"><%= subj[0].teacherName %></h2>
                            <div class="card">
                                <div class="table-responsive">
                                    <table class="table table-borderless table-header-bg text-center m-b-0">
                                        <thead>
                                            <tr>
                                                <th class="w-15"></th>
                                                <th class="text-center">1</th>
                                                <th class="text-center">2</th>
                                                <th class="text-center">3</th>
                                                <th class="text-center">4</th>
                                                <th class="text-center">5</th>
                                                <th class="text-center">6</th>
                                                <th class="text-center">7</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                              <td></td>
                                              <td></td>
                                              <td></td>
                                              <td></td>
                                              <td></td>
                                              <td></td>
                                              <td></td>
                                              <% for(var i = 0; i< 35; i++){%>
                                                <%if(i== 0 || i%7 == 0){%>
                                                </tr>
                                                <tr>
                                                  <td class="font-500 text-left" id="t<%=i%>"></td>
                                                  <script type="text/javascript">
                                                  console.log('<%=i/7%>');
                                                    switch ('<%=i/7%>') {
                                                      case '0':
                                                        $('#t<%=i%>').html('Monday')
                                                        break;
                                                      case '1':
                                                        $('#t<%=i%>').html('Thursday')
                                                        break;
                                                      case '2':
                                                        $('#t<%=i%>').html('Wednesday')
                                                        break;
                                                      case '3':
                                                        $('#t<%=i%>').html('Tuesday')
                                                        break;
                                                      case '4':
                                                        $('#t<%=i%>').html('Friday')
                                                        break;
                                                      default:

                                                    }
                                                  </script>
                                                <%}%>
                                                <% var status = true; for(var k =0; k < subj.length; k++){ %>
                                                  <% if( i == ((7*(Number(subj[k].day)-1))+Number(subj[k].time))-1){ console.log(((7*(Number(subj[k].day)-1))+Number(subj[k].time))-1, i); status = false;%>
                                                    <td><%= subj[k].subname %></td>
                                                  <%}%>
                                                <%}%>
                                                <%if(status){%>
                                                  <td></td>
                                                <%}%>


                                              <%}%>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                                <!-- .card-block -->
                            </div>
                            <!-- .card -->
                            <!-- End Classic table -->
                            <!-- End Introduction -->



                            <!-- Payments -->

                        </div>
                    </div>
                    <!-- End Section Content -->
                </div>
            </div>
                    <!-- End Categories -->

                </main>

                <footer class="app-layout-footer">
                    <div class="container p-y-md">
                        <div class="pull-right hidden-sm hidden-xs">
                            <a href="https://shapebootstrap.net/item/1525731-appui-admin-frontend-template/?ref=rustheme" target="_blank" rel="nofollow">Purchase a license</a>
                        </div>
                        <div class="pull-left text-center text-md-left">
                            AppUI &copy; <span class="js-year-copy"></span>
                        </div>
                    </div>
                </footer>

            </div>
            <!-- .app-layout-container -->
        </div>
        <!-- .app-layout-canvas -->

        <!-- Apps Modal -->
        <!-- Opens from the button in the header -->

        <!-- End Apps Modal -->

        <% include ../../partial/script %>

    </body>

</html>
